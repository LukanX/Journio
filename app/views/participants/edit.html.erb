<% provide(:title, 'Add a participant') %>
<h1>Update a participant</h1>
<div class="row">
	<div class="span6 offset3">
		<%= form_for [current_study, @participant] do |f| %>
			<%= render 'fields', f: f %>
			<%= f.submit "Update participant", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
