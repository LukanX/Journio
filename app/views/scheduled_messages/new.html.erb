<% provide(:title, 'Schedule a Message') %>
<h1>Schedule a Message</h1>
<div class="row">
	<div class="span6 offset3">
		<%= form_for [current_study, @scheduled_message] do |f| %>
			<%= render 'fields', f: f %>
			<%= f.submit "Schedule Message", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
